<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="329.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep8.controller.CustomerFormController">
   <children>
      <Label layoutX="34.0" layoutY="28.0" text="Report with Parameters">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <VBox layoutX="21.0" layoutY="70.0" prefHeight="224.0" prefWidth="290.0" spacing="10.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
         <children>
            <Label alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="30.0" prefWidth="290.0" text="Customer ID" />
            <TextField fx:id="txtID" alignment="CENTER" promptText="Eg. C001" />
            <Label alignment="CENTER" layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" prefHeight="30.0" prefWidth="290.0" text="Customer Name" />
            <TextField fx:id="txtName" alignment="CENTER" layoutX="10.0" layoutY="40.0" promptText="Eg. Dulanga" />
            <Label alignment="CENTER" layoutX="10.0" layoutY="66.0" maxWidth="1.7976931348623157E308" prefHeight="30.0" prefWidth="290.0" text="Customer Address" />
            <TextField fx:id="txtAddress" alignment="CENTER" layoutX="10.0" layoutY="96.0" promptText="Eg. Galle Road, Panadura" />
            <FlowPane alignment="CENTER">
               <children>
                  <Button defaultButton="true" mnemonicParsing="false" onAction="#btnShowReport_OnAction" text="Show Report" />
               </children>
            </FlowPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
